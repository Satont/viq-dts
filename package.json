    "m3u8": "^0.0.10",
    "m3u8-parser": "^4.7.1",
    "m3u8stream": "^0.8.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
